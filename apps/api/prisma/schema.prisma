generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id               String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  email            String    @unique
  firstName        String?
  lastName         String?
  stripeCustomerId String?
  emailVerifiedAt  DateTime?
  createdAt        DateTime? @default(now())
}
